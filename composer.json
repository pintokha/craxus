{
    "name": "pintokha/craxus",
    "description": "A package to send your test result on your Craxus account",
    "keywords": ["testing", "manager", "phpunit", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sviatoslav Pintokha",
            "email": "pintokha17@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.3",
        "phpunit/phpunit": ">=7.2"
    },
    "autoload": {
        "psr-4": {
            "Pintokha\\Craxus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pintokha\\Craxus\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
