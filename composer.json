{
    "name": "pintokha/craxus",
    "description": "PHP library for interacting with the Craxus service.",
    "keywords": ["phpunit", "php", "laravel", "testing", "manager"],
    "homepage": "https://github.com/pintokha/craxus",
    "license": "MIT",
    "authors": [
        {
            "name": "Sviatoslav Pintokha",
            "email": "pintokha17@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.2"
    },
    "autoload": {
        "psr-4": {
            "Pintokha\\Craxus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pintokha\\Craxus\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
